pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                script {
                    sh "This is build"
                }
            }
        }
        stage('Test') {
            steps {
                script {
                    sh "This is TEST"
                }
            }
        }
        stage('Deploy') {
            steps {
                script {
                    sh "This is deploy"
                }
            }
        }
    }
}